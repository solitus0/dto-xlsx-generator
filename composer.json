{
    "name": "solitus0/dto-xlsx-generator",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "A lightweight Symfony bundle for generating XLSX files from typed DTOs.",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "doctrine/common": "^3.5",
        "phpoffice/phpspreadsheet": "^5.0",
        "symfony/dependency-injection": "^5.4|^6.4|^7.0",
        "symfony/framework-bundle": "^5.4|^6.4|^7.0",
        "symfony/property-info": "^5.4|^6.4|^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Solitus0\\DtoXlsxGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solitus0\\DtoXlsxGenerator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "symplify/easy-coding-standard": "^12.5",
        "phpstan/phpstan": "^2.1.6",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "spaze/phpstan-disallowed-calls": "^4.5"
    }
}
